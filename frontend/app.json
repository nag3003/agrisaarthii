{
    "expo": {
        "name": "AgriSaarthi",
        "slug": "agrisarathi-frontend",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.agrisarathi.app"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.agrisarathi.app"
        },
        "web": {
            "favicon": "./assets/favicon.png",
            "bundler": "metro"
        },
        "scheme": "agrisarathi",
        "plugins": [
            "expo-router",
            [
                "expo-build-properties",
                {
                    "android": {
                        "compileSdkVersion": 34,
                        "targetSdkVersion": 34,
                        "buildToolsVersion": "34.0.0"
                    },
                    "ios": {
                        "useFrameworks": "static"
                    }
                }
            ]
        ]
    }
}